{
	"name": "openimu-core-libraries",
  "version": "1.0.22",
 	"description": "Open Source Embedded IMU and Navigation Library",
  "keywords": "sensors, IMU, INS, AI, automotive, navigation",
  "repository":
  {
    "type": "git",
    "url": "https://github.com/Aceinna/openimu-core.git",
    "branch": "develop"
  },
 	"build": {
 		"flags": [
 			"-I Common/include",
 			"-I BSP/include",
 			"-I BSP",
 			"-I CSL",
